{
  "buildCommand": "bash scripts/build-for-vercel.sh",
  "outputDirectory": "dist",
  "installCommand": "npm install && cd ../../packages/utopia && npm install",
  "framework": "vite",
  "devCommand": "npm run dev",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "github": {
    "silent": true
  }
}