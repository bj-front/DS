{
  "buildCommand": "cd ../.. && npx turbo build --filter=...utopia-website",
  "outputDirectory": "dist",
  "installCommand": "cd ../.. && npm ci",
  "framework": "vite",
  "devCommand": "npm run dev",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "github": {
    "silent": true
  }
}